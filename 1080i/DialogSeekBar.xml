                    <texture border="2" flipx="true">Seekbar/OSDPlayNF.png</texture>
                </control>
                <control type="image" description="RFFF">
                    <visible>player.rewinding4x | player.rewinding8x | player.rewinding16x | player.rewinding32x</visible>
