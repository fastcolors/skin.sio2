                    <left>10</left>
                    <include>OSDbarIcons</include>
                    <texture border="2">Seekbar/OSDPlayNF.png</texture>
                </control>
                <control type="image" description="4FFFF">
                    <visible>player.forwarding16x | player.forwarding32x</visible>
                    <left>20</left>
                    <include>OSDbarIcons</include>
                    <texture border="2">Seekbar/OSDPlayNF.png</texture>
